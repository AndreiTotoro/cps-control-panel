generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model volume {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  link  String
  titlu String
}

model anunturi {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  continut  String
  titlu String
}

model premii {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  link  String
  titlu String
}

model regulamente {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  link  String
  titlu String
}